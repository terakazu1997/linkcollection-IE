const  defaultFileLinks =[
        {linkTitle: "リンクタイトル",anchorLink:"リンク先名",imgSrc: "img/karaage.jpg"},
        {linkTitle: "リンクタイトル",anchorLink:"リンク先名,imgSrc: "img/pan.jpg"},
]

var createAllLinks = function (siteLinks,folderLinks,fileLinks){
        var allLinks = [];
        for(var i =0;i<siteLinks.length;i++){
            allLinks.push(siteLinks[i]);
        }
        for(var i =0;i<folderLinks.length;i++){
            allLinks.push(folderLinks[i]);
        }
        for(var i =0;i<fileLinks.length;i++){
            allLinks.push(fileLinks[i]);
        }
        return allLinks
    }